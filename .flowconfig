[ignore]
./dist
[include]

[libs]
./flow-typed
[lints]

[options]
module.system.node.resolve_dirname=./src
module.system.node.resolve_dirname=node_modules

module.name_mapper.extension='css' -> '<PROJECT_ROOT>/CSSFlowStub.js.flow'
